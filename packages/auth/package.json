{
  "name": "@letscollab/service-auth",
  "version": "9.1.0",
  "description": "",
  "types": "./src/domain/index",
  "private": true,
  "dependencies": {
    "@letscollab/midway-nacos": "workspace:^",
    "@midwayjs/bootstrap": "link:../../third_party/midway/packages/bootstrap",
    "@midwayjs/core": "link:../../third_party/midway/packages/core",
    "@midwayjs/decorator": "link:../../third_party/midway/packages/decorator",
    "@midwayjs/express": "link:../../third_party/midway/packages/web-express",
    "@midwayjs/orm": "link:../../third_party/midway/packages/orm",
    "mysql2": "^2.3.3",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@midwayjs/bundle-helper": "^1.0.2",
    "@midwayjs/cli": "1.3.0",
    "@midwayjs/mock": "link:../../third_party/midway/packages/mock",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.10",
    "@types/node": "16.11.26",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "mwts": "^1.3.0",
    "pm2": "^5.1.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "boot": "cross-env NODE_ENV=production pm2-runtime ./bootstrap.js",
    "start": "cross-env NODE_ENV=development nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./bootstrap.js",
    "mw": "cross-env NODE_ENV=development midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "ncc": "bundle && npm run build && ncc build bootstrap.js -o build",
    "bundle": "bundle",
    "generate": "tsproto --path proto --output src/domain"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
