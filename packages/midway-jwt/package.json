{
  "name": "@deskbtm/midway-jwt",
  "version": "0.0.1-alpha.15",
  "description": "midway jwt",
  "main": "src/index.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "main": "index.js",
    "typings": "index.d.ts",
    "directory": "dist"
  },
  "scripts": {
    "start": "midway-bin dev -f ./src/index.ts",
    "build": "midway-bin build -c",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "prepack": "rimraf dist && npm run build && ncp README.md dist/README.md && ncp package.json dist/package.json && ncp LICENSE dist/LICENSE",
    "postpublish": "rimraf dist",
    "ver": "standard-version"
  },
  "keywords": [
    "midwayjs",
    "midway-jwt",
    "jwt"
  ],
  "author": "Nawbc",
  "repository": {
    "url": "https://github.com/Nawbc/megumin-mgmt"
  },
  "license": "MIT",
  "devDependencies": {
    "@midwayjs/cli": "^1.2.38",
    "@midwayjs/core": "^2.3.0",
    "@midwayjs/decorator": "^2.3.0",
    "@midwayjs/express": "^2.13.3",
    "@midwayjs/koa": "^2.13.3",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "14",
    "@types/passport": "^1.0.7",
    "cross-env": "^6.0.0",
    "jest": "^26.4.0",
    "mwts": "^1.0.5",
    "standard-version": "^9.3.2",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1"
  },
  "peerDependencies": {
    "@midwayjs/express": "^2.13.3",
    "@midwayjs/koa": "^2.13.3"
  }
}
